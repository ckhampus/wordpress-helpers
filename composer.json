{
    "name": "queensbridge/wordpress-helpers",
    "description": "Helper classes and functions for WordPress",
    "authors": [
        {
            "name": "Cristian Hampus",
            "email": "cristian@queensbridge.se"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/form": "v2.1.1",
        "symfony/validator": "v2.1.1",
        "symfony/translation": "v2.1.1",
        "symfony/serializer": "v2.1.1",
        "symfony/config": "v2.1.1",
        "symfony/locale": "v2.1.1",
        "symfony/http-foundation": "v2.1.1",
        "symfony/twig-bridge": "v2.1.1",
        "twig/twig": "dev-master",
        "silex/silex": "dev-master"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "dev-master",
        "ckhampus/wordpress-tests": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Queensbridge": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://repo.cristianhampus.se"
        },
        {
            "type": "package",
            "package": {
                "name": "ckhampus/wordpress-tests",
                "type": "library",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/ckhampus/wordpress-tests.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
